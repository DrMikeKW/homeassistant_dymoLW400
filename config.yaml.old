name: "Dymo Label Printer"
version: "1.0.0"
slug: "dymo_printer"
description: "Print labels on Dymo LabelWriter 400 via HTTP requests"
init: false
arch:
  - aarch64
  - amd64
  - armhf
  - armv7
  - i386
startup: application
boot: auto
usb: true
full_access: true
devices:
  - /dev/bus/usb
ports:
  8000/tcp: 8000
ports_description:
  8000/tcp: Web interface
map:
  - config:rw